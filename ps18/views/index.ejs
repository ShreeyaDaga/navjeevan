<!DOCTYPE html>
<html>
<head>
  <title>Music</title>
</head>
<body>
  <h1><%= title %></h1>
  <p>Count: <%= count %></p>
  <table border="1" style="padding: 5px">
    <tr>
      <th>Song</th>
      <th>Film</th>
      <th>Music Director</th>
      <th>Singer</th>
      <th>Actor</th>
      <th>Actress</th>
    </tr>
    <% songs.forEach(song => { %>
      <tr>
        <td><%= song.Songname %></td>
        <td><%= song.Film %></td>
        <td><%= song.Music_director %></td>
        <td><%= song.Singer %></td>
        <td><%= song.Actor || '' %></td>
        <td><%= song.Actress || '' %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>